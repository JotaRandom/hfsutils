#!/bin/sh
# Minimal configure script for hfsck
echo "Configuring hfsck..."

cat > config.h <<'EOF'
/* config.h for hfsck - minimal autogenerated */
#define HAVE_CONFIG_H 1
/* Add other feature detects here if needed */
EOF

cat > config.status <<'EOF'
# config.status for hfsck - touch file to indicate configured
EOF

chmod +x config.status
echo "Configuration complete."
exit 0
